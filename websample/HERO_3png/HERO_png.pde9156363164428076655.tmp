int stert = 0;
float A = 0;
int B = 0;
PImage bim;
PImage HERO;

float x;
float y;


void setup() {
  size(320, 480);
  //noCursor();
  if (stert < 1) {
  }
}
void draw() {
  x = mouseX;
  y = mouseY;
  if (stert > 1) {
    background(100,100,100);
     
HERO = loadImage("HERO.png");



image(HERO,mouseX-25 ,mouseY-25,50,50);
bim = loadImage("bim.png");
image(bim,x,y);
  }
}
void mousePressed() {
  stert += 1;
}
